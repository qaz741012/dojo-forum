<div class="w-100">
  <div class="feeds-up pt-5 text-center">
    <div class="d-inline-block">
      <h2>User</h2>
      <div class="feeds-number-box text-center">
        <p><%= @user_count %></p>
      </div>
    </div>
    <div class="d-inline-block">
      <h2>Post</h2>
      <div class="feeds-number-box text-center">
        <p><%= @post_count %></p>
      </div>
    </div>
    <div class="d-inline-block">
      <h2>Comment</h2>
      <div class="feeds-number-box text-center">
        <p><%= @reply_count %></p>
      </div>
    </div>
  </div>
  <div class="feeds-down">
    <div class="">

    </div>
    <div class="">

    </div>
  </div>
</div>
